{
  "name": "extra-npm",
  "version": "2.0.28",
  "description": "npm is the default package manager for Node.js.",
  "main": "index.js",
  "module": "index.mjs",
  "exports": {
    "require": "./index.js",
    "import": "./index.mjs"
  },
  "preferGlobal": true,
  "bin": {
